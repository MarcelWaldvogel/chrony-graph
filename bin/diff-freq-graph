#!/bin/bash
TERMINAL=$1

LINSCALELIMIT=1
for stat in statistics.*; do
    LINSCALELIMIT=`../bin/linscalelimit $LINSCALELIMIT 5 $stat`
    echo $stat $LINSCALELIMIT
done
LINSCALELIMIT=`echo $LINSCALELIMIT | ../bin/mul 1000000`

# echo $LINSCALELIMIT

TERMINAL=$1
(
FORCE_LINEAR=${FORCE_LINEAR_ALLSTAR} ../bin/graph-header all-diff-freq $TERMINAL $LINSCALELIMIT "%'1.1f Âµs/s" "All Clocks (Frequency Differences)" "set yrange [-3:3]"
../bin/foreach-stat "'__FILE__' using 1:(\$6*1000000) title '__TITLE__' with line \\"
echo 
) | gnuplot
